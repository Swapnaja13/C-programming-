/******************************************************************************
                            POWER OF TWO
*******************************************************************************/

#include <stdio.h>

int main()
{
    int num;
    scanf("%d",&num);
    if(num&(num-1))
    printf("Num is not a power of two\n");
    else
    printf("Num is power of two\n");
    return 0;
}

