/******************************************************************************

                           SINGLE OCCURANCE WITH OTHERS TWICE OCCRANCE

*******************************************************************************/

#include <stdio.h>

int main()
{
    int arr[]={2,2,5,5,4,4,3};
    int res=0;
    int n=sizeof(arr)/sizeof(arr[0]);
    for(int i=0;i<n;i++)
    res=res^(arr[i]);   00->0 01->1 10->1 11->0 
    printf("%d",res);
}
