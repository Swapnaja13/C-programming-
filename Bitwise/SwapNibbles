/******************************************************************************
                            SWAPNIBBLES
*******************************************************************************/

#include <stdio.h>

int main()
{
    unsigned char X=0x0F; //X=100
    
    X=((X&0x0F)<<4)|((X&0XF0)>>4);
    printf("%X",X);   //printf("%d",X);   

    return 0;
}
