/******************************************************************************

                            SWAP INTEGER NUMBER

*******************************************************************************/

#include <stdio.h>

int main()
{
    int num=0x12345678;
    long int temp=0
    while(num)
    {
        temp|=num&1;
        temp=temp<<1;
        num=num>>1;
    }
    temp=temp>>1;
    printf("%d",temp);
    
}
